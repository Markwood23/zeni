# Zeni App Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Key for Zai AI Assistant
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Alternative: Use EXPO_PUBLIC_ prefix for client-side access
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# EAS Build Project ID (optional, for Expo Application Services)
EAS_PROJECT_ID=your_eas_project_id_here
